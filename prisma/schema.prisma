datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String @unique
  password String
  createdAt DateTime @default(now())
}

model Produtos {
  id    String @id @default(uuid())
  name  String
  preco Int
}